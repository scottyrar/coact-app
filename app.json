{
  "expo": {
    "name": "coact",
    "slug": "coact-app",
    "scheme": "coact",
    "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#31C48D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.scotty.rar.coactapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffff"
      },
      "package": "com.scotty.rar.coactapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-updates"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a45c691b-020b-4a0c-ad6c-9fd494c9f30d"
      }
    }
  }
}
